{
  "name": "nlua",
  "version": "1.0.0",
  "description": "A tool for calling Lua scripts from Node.js",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "gypfile": true,
  "scripts": {
    "test1": "node ./test/test1.js",
    "test1cjs": "node ./test/test1.cjs",
    "test2": "node ./test/test2.js",
    "test2cjs": "node ./test/test2.cjs",
    "build": "node-gyp rebuild",
    "clean": "node-gyp clean",
    "dev": "node-gyp clean && node-gyp rebuild"
  },
  "keywords": [
    "lua"
  ],
  "author": "milad.ashjari",
  "license": "ISC",
  "devDependencies": {
    "node-gyp": "^9.3.1"
  },
  "dependencies": {
    "node-addon-api": "^5.1.0"
  },
  "homepage": "http://github.com/MiladA555/nlua",
  "repository": {
    "type": "git",
    "url": "git@github.com:MiladA555/nlua.git"
  }
}
